package Diretorio;

public class CriarDiretorios {
public static void main(String[] args) {
Diretorio d1 = new Diretorio();
Diretorio d2 = new Diretorio();
Diretorio d3 = new Diretorio();
Diretorio d4 = new Diretorio();
	
System.out.println("Quantide de D1:" + d1.quantidade);
System.out.println("Quantide de D2:" + d2.quantidade);
System.out.println("Quantide de D3:" + d3.quantidade);
System.out.println("Quantide de D4:" + d4.quantidade);
System.out.println("Quantide de D4:" + Diretorio.quantidade);

}

}
